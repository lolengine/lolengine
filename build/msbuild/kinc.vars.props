<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />

  <PropertyGroup Label="UserMacros">
    <KincIncludes>
      $(KINC_SDK_PATH)\Sources
    </KincIncludes>
    <KincDefines>
      LOL_USE_KINC;
      KINC_G1;KINC_G2;KINC_G3;KINC_A1;KINC_A2;KINC_LZ4X;KINC_G4;KINC_G5;
    </KincDefines>
  </PropertyGroup>

  <PropertyGroup Label="UserMacros" Condition="'$(Platform)'=='Win32' or '$(Platform)'=='x64' or '$(Platform)'=='ARM64'">
    <ShaderCompiler>$(KINC_SDK_PATH)\Tools\windows_x64\krafix.exe</ShaderCompiler>
    <ShaderPlatform>windows</ShaderPlatform>
    <ShaderProfile>d3d11</ShaderProfile>
    <KincIncludes>
      $(KincIncludes);
      $(KINC_SDK_PATH)\Backends\System\Windows\Sources;
      $(KINC_SDK_PATH)\Backends\System\Microsoft\Sources;
      $(KINC_SDK_PATH)\Backends\System\Windows\Libraries\DirectShow\BaseClasses;
      $(KINC_SDK_PATH)\Backends\Graphics4\Direct3D11\Sources;
      $(KINC_SDK_PATH)\Backends\Audio2\WASAPI\Sources;
      $(KINC_SDK_PATH)\Backends\Graphics5\G5onG4\Sources;
    </KincIncludes>
    <KincIncludes Condition="'$(KincUseSteamworks)'=='true'">
      $(KincIncludes);
      $(KINC_SDK_PATH)\Backends\Service\Steam\Sources;
      $(STEAM_SDK_PATH)\public;
    </KincIncludes>
    <KincDefines>
      $(KincDefines);
      KINC_DIRECT3D;KINC_DIRECT3D11;KINC_G5ONG4;KINC_NO_MAIN;
    </KincDefines>
    <KincDefines Condition="'$(KincUseSteamworks)'=='true'">
      $(KincDefines);
      KINC_USE_STEAM;STEAMAPPID=$(KincSteamAppId);
    </KincDefines>
    <KincDeps>
      winhttp.lib;d3d11.lib;dxguid.lib;dinput8.lib;strmiids.lib;winmm.lib;wbemuuid.lib;
    </KincDeps>
    <KincDeps Condition="'$(KincUseSteamworks)'=='true'">
      $(KincDeps);
      $(STEAM_SDK_PATH)\redistributable_bin\win64\steam_api64.lib;
    </KincDeps>
  </PropertyGroup>

  <PropertyGroup Label="UserMacros" Condition="'$(Platform)'=='Gaming.Xbox.XboxOne.x64'">
    <ShaderCompiler>$(KINC_SDK_PATH)\..\Backends\Xbox\Tools\krafix-xboxone.exe</ShaderCompiler>
    <ShaderPlatform>xboxone</ShaderPlatform>
    <ShaderProfile>xboxone</ShaderProfile>
    <KincIncludes>
      $(KincIncludes);
      $(KINC_SDK_PATH)\Backends\System\Microsoft\Sources;
      $(KINC_SDK_PATH)\Backends\Graphics4\G4onG5\Sources;
      $(KINC_SDK_PATH)\Backends\Graphics5\Direct3D12\Sources;
      $(KINC_SDK_PATH)\..\Backends\Xbox\Sources;
    </KincIncludes>
    <KincDefines>
      $(KincDefines);
      KINC_DIRECT3D;KINC_DIRECT3D12;KINC_DXC;KINC_DIRECT3D_HAS_NO_SWAPCHAIN;KINC_G4ONG5;KINC_XBOX_ONE;KINC_CONSOLE;
    </KincDefines>
    <KincDeps>
    </KincDeps>
  </PropertyGroup>

  <PropertyGroup Label="UserMacros" Condition="'$(Platform)'=='Gaming.Xbox.Scarlett.x64'">
    <ShaderCompiler>$(KINC_SDK_PATH)\..\Backends\Xbox\Tools\krafix-xboxseries.exe</ShaderCompiler>
    <ShaderPlatform>xboxseries</ShaderPlatform>
    <ShaderProfile>xboxseries</ShaderProfile>
    <KincIncludes>
      $(KincIncludes);
      $(KINC_SDK_PATH)\Backends\System\Microsoft\Sources;
      $(KINC_SDK_PATH)\Backends\Graphics4\G4onG5\Sources;
      $(KINC_SDK_PATH)\Backends\Graphics5\Direct3D12\Sources;
      $(KINC_SDK_PATH)\..\Backends\Xbox\Sources;
    </KincIncludes>
    <KincDefines>
      $(KincDefines);
      KINC_DIRECT3D;KINC_DIRECT3D12;KINC_DXC;KINC_DIRECT3D_HAS_NO_SWAPCHAIN;KINC_G4ONG5;KINC_XBOX_SERIES;KINC_CONSOLE;
    </KincDefines>
    <KincDeps>
    </KincDeps>
  </PropertyGroup>

  <PropertyGroup Label="UserMacros" Condition="'$(Platform)'=='NX32' Or '$(Platform)'=='NX64'">
    <ShaderCompiler>$(KINC_SDK_PATH)\..\Backends\Switch\Tools\krafix-switch.exe</ShaderCompiler>
    <ShaderPlatform>switch</ShaderPlatform>
    <ShaderProfile>switch</ShaderProfile>
    <KincIncludes>
      $(KincIncludes);
      $(KINC_SDK_PATH)\Backends\Graphics4\G4onG5\Sources;
      $(KINC_SDK_PATH)\..\Backends\Switch\Sources;
    </KincIncludes>
    <KincDefines>
      $(KincDefines);
      KINC_G4ONG5;KINC_SWITCH;KINC_CONSOLE;
    </KincDefines>
    <KincDeps>$(KincDeps);-lnn_profiler;</KincDeps>
  </PropertyGroup>

  <PropertyGroup Label="UserMacros" Condition="'$(Platform)'=='ORBIS'">
    <ShaderCompiler>$(KINC_SDK_PATH)\..\Backends\PlayStation4\Tools\krafix-ps4.exe</ShaderCompiler>
    <ShaderPlatform>ps4</ShaderPlatform>
    <ShaderProfile>ps4</ShaderProfile>
    <KincIncludes>
      $(KincIncludes);
      $(KINC_SDK_PATH)\Backends\Graphics4\G4onG5\Sources;
      $(KINC_SDK_PATH)\..\Backends\PlayStation4\Sources;
    </KincIncludes>
    <KincDefines>
      $(KincDefines);
      KINC_G4ONG5;KINC_PLAYSTATION_4;KINC_PS4;KINC_SONY;KINC_CONSOLE;
    </KincDefines>
    <KincDeps>
      -lSceGnm;-lSceGnmx;-lSceSaveDataDialog_stub_weak;-lSceCommonDialog_stub_weak;-lSceSysmodule_stub_weak;-lSceNpTrophy_stub_weak;-lSceSystemService_stub_weak;-lSceSaveData_stub_weak;-lSceShaderBinary;-lSceAudioOut_stub_weak;-lSceVideoOut_stub_weak;-lSceGpuAddress;-lSceGnmDriver_stub_weak;-lScePad_stub_weak;-lSceUserService_stub_weak
    </KincDeps>
    <KincLibs>
      $(KincLibs);
      $(KINC_SDK_PATH)/../Backends/PlayStation4/Libraries;
    </KincLibs>
  </PropertyGroup>

  <PropertyGroup Label="UserMacros" Condition="'$(Platform)'=='Prospero'">
    <ShaderCompiler>$(KINC_SDK_PATH)\..\Backends\PlayStation5\Tools\krafix-ps5.exe</ShaderCompiler>
    <ShaderPlatform>ps5</ShaderPlatform>
    <ShaderProfile>ps5</ShaderProfile>
    <KincIncludes>
      $(KincIncludes);
      $(KINC_SDK_PATH)\Backends\Graphics4\G4onG5\Sources;
      $(KINC_SDK_PATH)\..\Backends\PlayStation5\Sources;
    </KincIncludes>
    <KincDefines>
      $(KincDefines);
      KINC_G4ONG5;KINC_PLAYSTATION_5;KINC_PS5;KINC_SONY;KINC_CONSOLE;
    </KincDefines>
    <KincDeps>
      -lSceAgc;-lSceAgcCore;-lSceAgcGpuAddress;-lSceAgcDriver_stub_weak;-lSceAgc_stub_weak;-lSceNpGameIntent_stub_weak;-lSceSaveDataDialog_stub_weak;-lSceCommonDialog_stub_weak;-lSceNpUniversalDataSystem_stub_weak;-lSceSysmodule_stub_weak;-lSceSystemService_stub_weak;-lSceSaveData_stub_weak;-lSceAudioOut_stub_weak;-lSceVideoOut_stub_weak;-lScePad_stub_weak;-lSceUserService_stub_weak
    </KincDeps>
  </PropertyGroup>

  <ItemGroup Label="UserMacros">
    <BuildMacro Include="KincDeps"><Value>$(KincDeps)</Value></BuildMacro>
    <BuildMacro Include="KincLibs"><Value>$(KincLibs)</Value></BuildMacro>
    <BuildMacro Include="KincDefines"><Value>$(KincDefines)</Value></BuildMacro>
    <BuildMacro Include="KincIncludes"><Value>$(KincIncludes)</Value></BuildMacro>
  </ItemGroup>

</Project>
